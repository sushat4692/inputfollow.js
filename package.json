{
  "name": "inputfollow.js",
  "type": "module",
  "version": "0.0.3",
  "description": "Input Validate, Input Limit etc.",
  "homepage": "https://github.com/sushat4692/inputfollow.js#readme",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "module": "dist/index.es.js",
  "browser": "dist/inputfollow.js",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "types",
    "dist"
  ],
  "keywords": [
    "front-end",
    "form",
    "typescript"
  ],
  "scripts": {
    "prebuild": "rimraf dist && rimraf types",
    "build": "rollup -c",
    "postbuild": "mv ./dist/types ./types",
    "demo": "http-server ./demo",
    "prepare": "run-s prepare:*",
    "prepare:demo": "npm run build && run-p prepare:demo:*",
    "prepare:demo:js": "cp -f ./dist/inputfollow.js ./demo/js/inputfollow.js",
    "prepare:demo:css": "sass demo/scss/style.scss demo/css/style.css --source-map",
    "prepare:doc": "api-extractor run --local --verbose && api-documenter markdown -i ./temp -o ./docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "SUSH <sush@sus-happy.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@microsoft/api-documenter": "^7.21.4",
    "@microsoft/api-extractor": "^7.34.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-server": "^14.1.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup": "^3.14.0",
    "sass": "^1.58.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "zod": "^3.20.5"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushat4692/inputfollow.js.git"
  },
  "bugs": {
    "url": "https://github.com/sushat4692/inputfollow.js/issues"
  }
}
